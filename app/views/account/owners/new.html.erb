<div class="col-md-4 col-md-offset-4">
  <h1>新增個人簡介</h1>
  <hr>

  <%= simple_form_for @account_owner, url: account_owner_path do |f| %>
    <div class="form-group">
      <%= f.input :content, input_html: {class: "form-control"} %>
    </div>
    <%= f.submit "Submit", class: "btn btn-primary", date: {disable_with: "Submitting..."}%>
  <% end %>
</div>
